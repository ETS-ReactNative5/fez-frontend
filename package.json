{
  "name": "espace-frontend",
  "version": "0.0.1",
  "main": "src/index.js",
  "license": "MIT",
  "devDependencies": {
    "axios-mock-adapter": "1.10.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "8.0.0",
    "babel-jest": "^22.4.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.8",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chalk": "^2.3.1",
    "chromedriver": "^2.33.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.10",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.18.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.10.1 ",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.4.2",
    "jest-cli": "^22.4.2",
    "jsdom": "^11.6.2",
    "jsdom-global": "^3.0.2",
    "node-sass": "^4.8.2",
    "postcss-loader": "^2.1.0",
    "preloader-html-webpack-plugin": "github:uqlibrary/preloader-html-webpack-plugin#0.0.2",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react-addons-test-utils": "^15.6.0",
    "react-tap-event-plugin": "^2.0.1",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.5.1",
    "sass-loader": "^6.0.6",
    "sinon": "^3.3.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^1.2.0",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-server": "^2.9.2"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "axios-cache-adapter": "^2.0.0",
    "babel-loader": "^7.1.4",
    "connected-react-router": "^4.3.0",
    "dompurify": "^1.0.3",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "js-cookie": "^2.2.0",
    "load-script": "^1.0.0",
    "material-ui": "^0.18.7",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "prop-types": "^15.6.0",
    "raven-js": "^3.23.2",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-dropzone": "^4.2.8",
    "react-google-maps": "^9.4.5",
    "react-hot-loader": "^3.1.3",
    "react-html-parser": "^2.0.2",
    "react-infinite": "^0.12.1",
    "react-json-tree": "^0.10.9",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-rte": "^0.16.0",
    "redux": "^3.7.2",
    "redux-form": "^7.2.3",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "uqlibrary-react-toolbox": "github:uqlibrary/uqlibrary-react-toolbox#master"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "start:mock": "cross-env USE_MOCK=true webpack-dev-server",
    "start:url": "cross-env URL=dev-espace.library.uq.edu.au webpack-dev-server",
    "build:dev": "webpack --config=./webpack.config",
    "build": "webpack --config=./webpack-dist.config",
    "analyse": "webpack --config=./webpack-dist.config --profile --json > stats.json",
    "start:build": "cross-env NODE_ENV=production URL=dev-espace.library.uq.edu.au webpack-dev-server --config=./webpack-dist.config --disable-host-check",
    "start:build:e2e": "cross-env NODE_ENV=production URL=localhost USE_MOCK=true webpack-dev-server --config=./webpack-dist.config",
    "test": "NODE_ENV=test jest",
    "test:watch": "npm run test -- --watch",
    "test:update": "npm run test -- --updateSnapshot",
    "eslint": "eslint 'src/**/*.js' --quiet"
  },
  "engines": {
    "npm": ">=4",
    "node": ">=7.2.0"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-0",
      "stage-1"
    ],
    "env": {
      "development": {
        "presets": [
          "env",
          "react",
          "stage-0",
          "stage-1"
        ]
      },
      "production": {
        "presets": [
          [
            "env",
            {
              "es2015": {
                "modules": false
              },
              "targets": {
                "ie": 11,
                "uglify": true
              }
            }
          ]
        ],
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-remove-prop-types",
          "transform-class-properties"
        ]
      },
      "test": {
        "presets": [
          "env",
          "react",
          "stage-0",
          "stage-1"
        ],
        "plugins": [
          [
            "dynamic-import-node",
            "transform-runtime",
            {
              "helpers": false,
              "polyfill": false,
              "regenerator": true,
              "moduleName": "babel-runtime"
            }
          ]
        ]
      }
    }
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "enzyme"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "html",
      "text"
    ],
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|sass)$": "<rootDir>/__mocks__/styles.js",
      "\\.(jpg|jpeg|svg)$": "<rootDir>/__mocks__/images.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "setupFiles": [
      "./src/test.setup.js"
    ],
    "testURL": "http://fez-staging.library.uq.edu.au"
  }
}
